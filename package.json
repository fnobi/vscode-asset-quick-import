{
	"name": "assetquickimport",
	"displayName": "AssetQuickImport",
	"description": "quick pick asset path & insert import statement.",
	"version": "0.0.6",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.assetquickimport"
	],
	"publisher": "fnobi",
	"repository": "https://github.com/fnobi/vscode-asset-quick-import",
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.assetquickimport",
				"title": "Asset Quick Import"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/find": "^0.2.1",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"@typescript-eslint/eslint-plugin": "^8.46.4",
		"@typescript-eslint/parser": "^8.46.4",
		"eslint": "^9.39.1",
		"glob": "^7.1.6",
		"mocha": "^10.8.2",
		"typescript": "^3.7.5",
		"vsce": "^2.7.0",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"find": "^0.3.0"
	}
}